<?xml version="1.0" encoding="utf-8"?>
<com.homeraria.hencodeuicourse.app.farmland.TargetSimpleLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#222">


    <com.homeraria.component.widget.ProgressBarCircularIndeterminate
        android:id="@+id/target"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_centerInParent="true"
        android:background="@color/md_grey_500" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="点击开始" />

</com.homeraria.hencodeuicourse.app.farmland.TargetSimpleLayout>